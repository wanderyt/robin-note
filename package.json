{
  "name": "robin-note",
  "version": "1.7.0",
  "description": "note board for little robin",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "m": "concurrently --kill-others-on-fail \"node server/server.js\" \"npm run start\"",
    "server": "node server/index.js",
    "server-debug": "node --inspect server/server.js",
    "mytest": "jest",
    "testApp": "TEST_ENV=true npm run m",
    "storybook": "node .story/index.js"
  },
  "engines": {
    "node": ">10.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wanderyt/robin-note.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wanderyt/robin-note/issues"
  },
  "dependencies": {
    "autoprefixer": "7.1.2",
    "axios": "^0.17.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.18.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "colors": "^1.1.2",
    "concurrently": "^3.5.1",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "material-ui": "^0.20.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "normalize.css": "^8.0.1",
    "object-assign": "4.1.1",
    "path": "^0.12.7",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "properties-parser": "^0.3.1",
    "react": "^16.0.0",
    "react-dom": "^16.4.1",
    "react-facebook-login": "^4.0.1",
    "react-lazyload": "^2.3.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "request": "^2.87.0",
    "sass": "^1.15.1",
    "sass-loader": "^6.0.6",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "underscore": "^1.8.3",
    "url-loader": "0.5.9",
    "webpack": "^3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3",
    "xhr": "^2.4.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.2",
    "@storybook/react": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "gulp": "^4.0.0",
    "gulp-nodemon": "^2.4.2",
    "react-dev-utils": "^4.2.3",
    "react-loadable": "^5.5.0",
    "react-scripts": "^2.1.2",
    "uglifyjs-webpack-plugin": "^2.0.1"
  }
}
